{
  "Laravel": {
    "headers": ["X-Powered-By: Laravel/([0-9.]+)|X-Powered-By: Laravel", "X-Framework: Laravel"],
    "html": [
      "csrf-token",
      "_token",
      "laravel",
      "app\\/Http",
      "Illuminate\\\\",
      "Session::",
      "Route::",
      "<!-- Laravel -->"
    ],
    "version_pattern": "Laravel/([0-9.]+)",
    "must_not_have": ["django", "rails"]
  },
  "Django": {
    "headers": ["X-Powered-By: Django/([0-9.]+)|X-Powered-By: Django", "X-Frame-Options: SAMEORIGIN"],
    "html": [
      "csrfmiddlewaretoken",
      "__admin__",
      "Django",
      "django",
      "/static/admin/",
      "/admin/jsi18n/",
      "<!-- django -->"
    ],
    "version_pattern": "Django/([0-9.]+)",
    "must_not_have": ["laravel", "rails"]
  },
  "Ruby on Rails": {
    "headers": ["X-Runtime", "X-Rails", "X-Request-Id", "X-Powered-By: Ruby on Rails/([0-9.]+)|X-Powered-By: Ruby"],
    "html": [
      "csrf-token",
      "rails",
      "data-remote=\"true\"",
      "data-method=",
      "action=\"/rails/",
      "<!-- Rails -->"
    ],
    "version_pattern": "Ruby on Rails/([0-9.]+)",
    "must_not_have": ["django", "laravel"]
  },
  "Express.js": {
    "headers": ["X-Powered-By: Express/([0-9.]+)|X-Powered-By: Express", "X-Powered-By: Express/([0-9.]+)|X-Powered-By: Express/"],
    "html": [
      "express",
      "Express",
      "__express__",
      "app\\.use\\(",
      "res\\.render",
      "<!-- express -->"
    ],
    "version_pattern": "Express/([0-9.]+)",
    "must_not_have": []
  },
  "Spring Boot": {
    "headers": ["X-Application-Context", "X-SpringBoot-([0-9.]+)|X-SpringBoot-"],
    "html": [
      "spring",
      "Spring",
      "thymeleaf",
      "@Controller",
      "@Service",
      "<!-- Spring Boot -->"
    ],
    "version_pattern": "SpringBoot-([0-9.]+)",
    "must_not_have": []
  },
  "Symfony": {
    "headers": ["X-Powered-By: Symfony/([0-9.]+)|X-Powered-By: Symfony", "X-Symfony-([0-9.]+)|X-Symfony-"],
    "html": [
      "symfony",
      "Symfony",
      "sf-",
      "/bundles/",
      "/web/bundles/",
      "<!-- Symfony -->"
    ],
    "version_pattern": "(?:Symfony|X-Symfony)-([0-9.]+)",
    "must_not_have": []
  },
  "Yii": {
    "headers": ["X-Powered-By: Yii Framework/([0-9.]+)|X-Powered-By: Yii Framework", "X-Yii-([0-9.]+)|X-Yii-"],
    "html": [
      "yii",
      "Yii",
      "yii\\.js",
      "yiiActiveForm",
      "<!-- Yii Framework -->"
    ],
    "version_pattern": "(?:Yii Framework|X-Yii)-([0-9.]+)",
    "must_not_have": []
  },
  "CodeIgniter": {
    "headers": ["X-Powered-By: CodeIgniter/([0-9.]+)|X-Powered-By: CodeIgniter", "CI-([0-9.]+)|CI-"],
    "html": [
      "codeigniter",
      "CodeIgniter",
      "ci_",
      "<!-- CodeIgniter -->"
    ],
    "version_pattern": "(?:CodeIgniter|CI)-([0-9.]+)",
    "must_not_have": []
  },
  "Flask": {
    "headers": ["X-Powered-By: Flask/([0-9.]+)|X-Powered-By: Flask", "Server: Werkzeug"],
    "html": [
      "flask",
      "Flask",
      "<!-- flask -->",
      "url_for\\("
    ],
    "version_pattern": "Flask/([0-9.]+)",
    "must_not_have": []
  }
}
